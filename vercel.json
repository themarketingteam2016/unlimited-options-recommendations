{
  "version": 2,
  "builds": [
    {
      "src": "web/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web/index.js"
    }
  ],
  "env": {
    "SHOPIFY_API_KEY": "@shopify_api_key",
    "SHOPIFY_API_SECRET": "@shopify_api_secret",
    "SCOPES": "@scopes",
    "HOST": "@host"
  }
}
